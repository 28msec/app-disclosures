{
    "apiVersion": "1.0",
    "swaggerVersion": "1.2",
    "basePath": "http://disclosures.xbrl.io/v1/_queries/public",
    "resourcePath": "",
    "produces": [
        "application/json"
    ],
    "apis": [
        {
            "path": "/Concepts.jq",
            "description": "Concepts",
            "operations": [
                {
                    "method": "POST",
                    "summary": "Retrieve concepts from a mapping list",
                    "nickname": "concepts",
                    "parameters": [
                        {
                            "name": "list",
                            "description": "The list name. If empty, all concepts will be returned.",
                            "required": false,
                            "type": "string",
                            "paramType": "query"
                        },
                        {
                            "name": "token",
                            "description": "The token of the current session (if accessing entities beyond DOW30)",
                            "required": false,
                            "type": "string",
                            "paramType": "query"
                        }
                    ],
                    "responseMessages": [
                        {
                            "code": 401,
                            "message": "Unauthorized: the specified project token is invalid, expired, or has insufficient privileges",
                            "responseModel": "Error"
                        },
                        {
                            "code": 500,
                            "message": "An internal error occurred during the processing of the request.",
                            "responseModel": "Error"
                        }
                    ],
                    "successMessages": [
                        {
                            "code": 200,
                            "message": "Returns concepts list."
                        }
                    ],
                    "examples": [
                        {
                            "title": "Get the concepts for Policies",
                            "request": "[API_URL]/_queries/public/Concepts.jq?_method=POST&list=Policies",
                            "response": "[ { Name: \"disc:PensionAndOtherPostretirementPlansPensionsPolicy\", \"Label\" : \"Pension and Other Postretirement Plans, Pensions, Policy\" }, ... ]"
                        }
                    ]
                }
            ]
        },
        {
            "path": "/Concept.jq",
            "description": "Concept",
            "operations": [
                {
                    "method": "POST",
                    "summary": "Retrieve a concept",
                    "nickname": "concept",
                    "parameters": [
                        {
                            "name": "concept",
                            "description": "The name of the concept.",
                            "required": true,
                            "type": "string",
                            "paramType": "query"
                        },
                        {
                            "name": "token",
                            "description": "The token of the current session (if accessing entities beyond DOW30)",
                            "required": false,
                            "type": "string",
                            "paramType": "query"
                        }
                    ],
                    "responseMessages": [
                        {
                            "code": 401,
                            "message": "Unauthorized: the specified project token is invalid, expired, or has insufficient privileges",
                            "responseModel": "Error"
                        },
                        {
                            "code": 500,
                            "message": "An internal error occurred during the processing of the request.",
                            "responseModel": "Error"
                        }
                    ],
                    "successMessages": [
                        {
                            "code": 200,
                            "message": "Returns the concept."
                        }
                    ],
                    "examples": [
                        {
                            "title": "Get the concept disc:PensionAndOtherPostretirementPlansPensionsPolicy",
                            "request": "[API_URL]/_queries/public/Concept.jq?_method=POST&concept=disc:PensionAndOtherPostretirementPlansPensionsPolicy",
                            "response": "{ Name: \"disc:PensionAndOtherPostretirementPlansPensionsPolicy\", \"Label\" : \"Pension and Other Postretirement Plans, Pensions, Policy\", \"IsAbstract\": false }"
                        }
                    ]
                }
            ]
        }
    ],
    "models": {
        "Error": {
            "id": "Error",
            "description": "Error information",
            "required": [
                "success",
                "description"
            ],
            "properties": {
                "success": {
                    "type": "boolean",
                    "description": "Whether the requests succeeded or failed."
                },
                "description": {
                    "type": "string",
                    "description": "A description of the error."
                }
            }
        }
    }
}
